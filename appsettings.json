{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
   "Routes": {
     "foo-route" : {
       "AuthorizePolicy": "CustomPolicy",
       "RateLimitPolicy": "fixed-100",
       "ClusterId": "foo-cluster",
       "Match": {
         "Path": "foo/{**catch-all}"
       },
       "Transforms": [
         { "PathRemovePrefix": "{**catch-all}" }
       ]
     },
     "dummy-route" : {
       "ClusterId": "dummy-cluster",
       "Match": {
         "Path": "dummy/{**catch-all}"
       },
       "Transforms": [
         { "PathRemovePrefix": "{**catch-all}" }
       ]
     },
     "bar-route" : {
       "ClusterId": "bar-cluster",
       "Match": {
         "Path": "bar/{**catch-all}"
       },
       "Transforms": [
         { "PathRemovePrefix": "{**catch-all}" }
       ]
     }
   },
   "Clusters": {
     "foo-cluster": {
       "Destinations": {
         "LoadBalancingPolicy": "RoundRobin",
         "destination1": {
           "Address": "https://foo-api:5100/"
         },
         "destination2": {
           "Address": "https://foo-api:5110/"
         }
       }
     },
     "dummy-cluster": {
       "Destinations": {
         "destination1": {
           "Address": "https://dummy-api:5200/"
         }
       }
     },
     "bar-cluster": {
       "Destinations": {
         "destination1": {
           "Address": "https://bar-api:5300/"
         }
       }
     }
   }
  }
}
